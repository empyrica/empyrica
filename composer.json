{
    "name": "empiriq/empiriq",
    "version": "0.0.1",
    "minimum-stability": "alpha",
    "prefer-stable": true,
    "description": "Lazy iterators for historical Binance data from https://data.binance.vision",
    "keywords": [
        "binifit",
        "binifit-environment",
        "binance",
        "binance-api",
        "binance-streams",
        "binance-websockets"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mikhail Gerasimov",
            "email": "migerasimoff@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.4",
        "ext-zip": "*",
        "ext-openssl": "*",
        "psr/event-dispatcher": "^1.0",
        "symfony/polyfill-php84": "^1.32",
        "symfony/serializer-pack": "^1.3",
        "symfony/yaml": "^7.3",
        "symfony/event-dispatcher": "^7.3",
        "mitoteam/jpgraph": "^10.4",
        "sqonk/phext-visualise": "^1.0",
        "sqonk/phext-plotlib": "^1.1",
        "imagine/imagine": "^1.5",
        "symfony/console": "^7.3",
        "symfony/options-resolver": "^7.3",
        "symfony/dotenv": "^7.3",
        "monolog/monolog": "^3.9",
        "symfony/cache": "^7.3",
        "react/event-loop": "^1.5",
        "ratchet/pawl": "^0.4.3",
        "react/async": "^4.3",
        "react/stream": "^1.4",
        "react/promise-timer": "^1.11",
        "react/http": "^1.11",
        "bramus/monolog-colored-line-formatter": "^3.1",
        "plesk/stdio-react": "^2.6",
        "guzzlehttp/guzzle": "^7.10",
        "symfony/dependency-injection": "^7.3",
        "symfony/config": "^7.3",
        "symfony/http-kernel": "^7.3",
        "symfony/monolog-bundle": "^3.10",
        "symfony/framework-bundle": "^7.3",
        "symfony/runtime": "^7.3"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.10",
        "phpunit/phpunit": "^10",
        "vimeo/psalm": "^6.13",
        "ramsey/conventional-commits": "^1.6",
        "captainhook/captainhook-phar": "^5.25"
    },
    "replace": {
        "empiriq/binance-backtrade-bundle": "self.version",
        "empiriq/binance-contracts": "self.version",
        "empiriq/binance-manager-bundle": "self.version",
        "empiriq/binance-papertrade-bundle": "self.version",
        "empiriq/binance-trade-bundle": "self.version",
        "empiriq/contracts": "self.version",
        "empiriq/server": "self.version"
    },
    "autoload": {
        "psr-4": {
            "Empiriq\\BinanceApiConnector\\": "packages/binance-trade-bundle",
            "Empiriq\\BinanceContracts\\": "packages/binance-contracts",
            "Empiriq\\BinanceHistoryConnector\\": "packages/binance-backtrade-bundle",
            "Empiriq\\BinanceManager\\": "packages/binance-manager-bundle",
            "Empiriq\\Contracts\\": "packages/contracts",
            "Empiriq\\Server\\": "packages/server",
            "App\\": "packages/playground/src"
        }
    },
    "scripts": {
        "phpcs": "./vendor/bin/phpcs --ignore=*/vendor/*,*/var/*,*/bin/* --standard=PSR12 --exclude=Generic.Files.LineLength packages",
        "phpunit": "./vendor/bin/phpunit",
        "psalm": "./vendor/bin/psalm",
        "message": "./vendor/bin/conventional-commits prepare",
        "post-autoload-dump": [
            "php -r \"if (!file_exists('packages/playground/vendor')) { symlink(__DIR__ . '/vendor', __DIR__ . '/packages/playground/vendor'); echo 'Symlink created: packages/vendor -> vendor'.PHP_EOL; } else { echo 'Symlink already exists'.PHP_EOL; }\""
        ],
        "changelog": "conventional-changelog",
        "release": "conventional-changelog --commit",
        "release:patch": "conventional-changelog --patch --commit",
        "release:minor": "conventional-changelog --minor --commit",
        "release:major": "conventional-changelog --major --commit"
    },
    "config": {
        "allow-plugins": {
            "symfony/runtime": true,
            "captainhook/captainhook-phar": true
        }
    },
    "extra": {
        "ramsey/conventional-commits": {
            "config": {
                "types": [
                    "feat",
                    "fix",
                    "build",
                    "chore",
                    "ci",
                    "docs",
                    "style",
                    "refactor",
                    "perf",
                    "test"
                ],
                "scopeRequired": false,
                "scopes": [
                    "binance-trade-bundle"
                ]
            }
        }
    }
}
